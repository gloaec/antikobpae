body, div, span, object, input, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, img, q, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, dialog, figure, footer, header, hgroup, nav, section {
	font: 16px/1 "SPThunderFoxRegular", Arial, Helvetica, sans-serif;
}

body, html {
	color: #636B75; 
	min-width: 980px;
	height: 100%;
}

#wrapper {
	margin: 0 auto;
	position: relative;
	min-height: 100%;
	overflow: hidden;
}

.toggle_btn {
	display: inline-block;
	/*padding: 6px 30px 6px 6px;
	width: 122px;*/
	background-position: 136px center no-repeat;
	background-repeat: no-repeat;
	background-image: url(<%= asset_path 'toggle_off16.png' %>);
}

.toggle_btn:hover {	background-image: url(<%= asset_path 'toggle_off_hover16.png' %>); }
.toggle_btn.on { background-image: url(<%= asset_path 'toggle_on16.png' %>); }
.toggle_btn.on:hover { background-image: url(<%= asset_path 'toggle_on_hover16.png' %>); }

h1 { font-size: 2em }
h2 { font-size: 1.8em }
h3 { font-size: 1.3em; text-shadow: 1px 1px 1px #FFF; color: #666 }

input.title {
	font-size: 1.3em;
}

input[type="text"], input[type="password"], input[type="url"], input[type="email"], input.text, input.title, textarea, select {
	margin: 0.5em 0;
	padding: 5px;
	border-radius:5px;
}

a {
